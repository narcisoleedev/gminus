int sum(int a, int b){
  return a + b;
}

int sub(int a, int b){
  return a - b;
}

int mul(int a, int b){
  return a * b;
}

int div(int a, int b){
  return a / b;
}

void main(void){
  int a;
  int b;
  int c[3];
  a = 5;
  b = input();
  c[0] = sum(a, b);
  c[1] = sub(a, b);
  output(mul(a, b));
  c[2] = div(a, b);
}